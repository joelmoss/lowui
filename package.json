{
  "name": "lowui",
  "version": "0.1.0",
  "description": "Low Level React Components and Hooks",
  "private": true,
  "engines": {
    "node": "20.4.0"
  },
  "packageManager": "pnpm@8.7.4",
  "scripts": {
    "test": "jest",
    "lint": "eslint packages --ext .jsx --ext .js"
  },
  "keywords": [
    "components",
    "react"
  ],
  "author": "Joel Moss",
  "homepage": "https://github.com/joelmoss/lowui",
  "repository": {
    "type": "git",
    "url": "https://github.com/joelmoss/lowui.git"
  },
  "bugs": {
    "url": "https://github.com/joelmoss/lowui/issues"
  },
  "license": "MIT",
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "jsdocSingleLineComment": false,
    "jsdocDescriptionWithDot": true,
    "jsdocSeparateReturnsFromParam": true,
    "plugins": [
      "prettier-plugin-jsdoc"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    }
  },
  "pnpm": {
    "overrides": {
      "react-is": "^18.2.0",
      "prop-types": "github:joelmoss/prop-types#esm",
      "prop-types-exact": "github:joelmoss/prop-types-exact"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@developwithstyle/postcss-custom-media": "^8.0.2",
    "@rails/actioncable": "^7.0.7-2",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.6.4",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "microbundle": "^0.15.1",
    "open-props": "^1.5.15",
    "postcss": "^8.4.29",
    "postcss-mixins": "^9.0.4",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  }
}
